{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "sql:default",
    "sql:allow-execute",
    "core:window:allow-create",
    "core:window:allow-set-enabled",
    "core:window:allow-destroy",
    "core:webview:allow-create-webview"
  ]
}
